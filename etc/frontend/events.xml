<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="controller_front_send_response_before">
        <observer name="gomage_navigation_before_send" instance="GoMage\Navigation\Observer\BeforeSend" />
    </event>
    <event name="layout_load_before">
        <observer name="gomage_layout_generate_blocks_after" instance="GoMage\Navigation\Observer\LoadLayout" />
    </event>
    <event name="catalog_product_collection_apply_limitations_after">
        <observer name="gomage_layout_generate_blocks_after" instance="GoMage\Navigation\Observer\LimitationAfter" />
    </event>
</config>
